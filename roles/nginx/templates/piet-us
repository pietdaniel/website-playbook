server {
  listen       80;
  server_name  piet.us;

  location / {
    root /var/www/html/;
    index index.html index.html;
  }

  error_page  404              /404.html;
  location = /40x.html {
    root   /var/www/html;
  }

  error_page   500 502 503 504  /50x.html;
  location = /50x.html {
    root  /var/www/html;
  }
}
